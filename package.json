{
  "name": "vite-sdk-ts",
  "version": "0.0.1",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "keywords": [
    "typescript"
  ],
  "description": "vite-sdk-ts is a dev template",
  "homepage": "https://next-dev-template.github.io/vite-sdk-ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/next-dev-template/vite-sdk-ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vitest",
    "dev:web": "vite",
    "build": "unbuild",
    "preview": "vite build",
    "test": "vitest",
    "prepare": "husky install",
    "eslint": "eslint src --ext .ts,.js,.vue --fix",
    "stylelint": "stylelint src/**/*.{vue,css,less,scss} --fix",
    "lint": "npm run eslint && npm run stylelint",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "eslint": "^8.15.0",
    "eslint-config-jammy": "^0.0.9",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.7",
    "vitest": "^0.12.6",
    "less": "^4.1.2",
    "stylelint": "^14.8.2",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.4",
    "pkg-get": "^0.0.2"
  }
}
